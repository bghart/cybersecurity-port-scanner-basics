import socket

s = socket.socket()
result = s.connect_ex(('google.com',80))

if result == 0:
    print("Door is Open(Port 80)")
else:
    print('Door is CLOSED')

s.close()

s = socket.socket()
result = s.connect_ex(("127.0.0.1",80))

if result == 0:
    print("Door is Open(Port 80)")
else:
    print('Door is CLOSED')

s.close()




